    "deploy:github": "npm run build:github && node deploy.js && node github-pages-settings-reminder.js",
    "deploy:firebase": "npm run build:firebase && firebase deploy",
    "deploy:both": "node deploy-dual.js",
    "deploy:dual": "node deploy-dual.js",
    "reminder": "node github-pages-settings-reminder.js",
    "fix-404": "cp build/index.html build/404.html"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^6.3.0",
    "javascript-obfuscator": "^4.1.1",
