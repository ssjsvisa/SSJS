  "scripts": {
    "start": "react-scripts start",
    "prebuild": "node remove-cname.js",
    "build": "react-scripts build",
    "postbuild": "node remove-cname.js",
    "build:github": "node scripts/set-homepage.js github && npm run build",
    "build:firebase": "node scripts/set-homepage.js firebase && npm run build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build:github",
